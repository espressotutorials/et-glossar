{
  "name": "et-glossar",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "extract-i18n": "ng extract-i18n et-glossar --format xlf --output-path src/locales",
    "xliffmerge": "./node_modules/ngx-i18nsupport/dist/xliffmerge",
    "translate-en": "poedit src/locales/messages-en.xlf"
  },
  "xliffmergeOptions": {
    "srcDir": "src/locales",
    "genDir": "src/locales",
    "defaultLanguage": "de",
    "languages": [
      "de",
      "en"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/material": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@espressotutorialsgmbh/espresso-tailwind-config": "^0.0.8",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/pro-solid-svg-icons": "^6.1.1",
    "date-fns": "^2.28.0",
    "inter-ui": "^3.19.3",
    "ngx-i18nsupport": "^0.17.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/localize": "^14.1.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/jasmine": "~4.0.0",
    "autoprefixer": "^10.4.7",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "typescript": "~4.7.2"
  }
}
